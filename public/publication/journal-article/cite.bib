@article{Wang2021,
abstract = {Poleward migrations of tropical cyclones have been observed globally, but their impact on coastal areas remains unclear. We investigated the change in global tropical cyclone activity in coastal regions over the period 1982–2018. We found that the distance of tropical cyclone maximum intensity to land has decreased by about 30 kilometers per decade, and that the annual frequency of global tropical cyclones increases with proximity to land by about two additional cyclones per decade. Trend analysis reveals a robust migration of tropical cyclone activity toward coasts, concurrent with poleward migration of cyclone locations as well as a statistically significant westward shift. This zonal shift of tropical cyclone tracks may be mainly driven by global zonal changes in environmental steering flow.},
author = {Wang, Shuai and Toumi, Ralf},
doi = {10.1126/science.abb9038},
file = {:Users/shuaiwang/Library/Application Support/Mendeley Desktop/Downloaded/Wang, Toumi - 2021 - Recent migration of tropical cyclones toward coasts.pdf:pdf;:Users/shuaiwang/Library/Application Support/Mendeley Desktop/Downloaded/Wang, Toumi - 2021 - Recent migration of tropical cyclones toward coasts(2).pdf:pdf},
isbn = {1223326500},
issn = {0036-8075},
journal = {Science},
month = {jan},
number = {6528},
pages = {514--517},
pmid = {33510027},
title = {{Recent migration of tropical cyclones toward coasts}},
url = {https://www.sciencemag.org/lookup/doi/10.1126/science.abb9038},
volume = {371},
year = {2021}
}
