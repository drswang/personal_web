month = {aug},
number = {8},
pages = {981--993},
title = {{Reduced Sensitivity of Tropical Cyclone Intensity and Size to Sea Surface Temperature in a Radiative-Convective Equilibrium Environment}},
url = {https://link.springer.com/content/pdf/10.1007%2Fs00376-018-7277-5.pdf http://link.springer.com/10.1007/s00376-018-7277-5},
